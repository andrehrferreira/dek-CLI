{
    "name": "@dek/cli",
    "version": "1.0.0",
    "description": "CLI for Dek",
    "main": "index.js",
    "scripts": {
        "test": "npm run build && mocha",
        "build": "babel src -s -D -d build --presets env",
        "dev": "babel-node src --presets env",
        "start": "npm run build && node build"
    },
    "bin": {
        "dek": "build/index.js"
    },
    "engines": {
        "node": ">= 6.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dekproject/CLI.git"
    },
    "keywords": [
        "CLI",
        "Dek"
    ],
    "author": "Andre Ferreira <andre@vigiadepreco.com.br>",
    "contributors": [
        "Eduardo Lima <eduardo@vigiadepreco.com.br>"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dekproject/CLI/issues"
    },
    "homepage": "https://github.com/dekproject/CLI",
    "dependencies": {
        "chalk": "^2.4.2",
        "clear": "^0.1.0",
        "figlet": "^1.2.1",
        "git-clone": "^0.1.0",
        "i18n": "^0.8.3",
        "inquirer": "^6.2.2",
        "inquirer-fuzzy-path": "^2.0.1",
        "lodash": "^4.17.11",
        "minimist": "^1.2.0",
        "npm": "^6.8.0",
        "rimraf": "^2.6.3",
        "react-dom": "^16.8.1",
        "react": "^16.8.1"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-minify": "^0.5.0",
        "babel-preset-react": "^6.24.1",
        "cross-env": "^5.2.0",
        "mocha": "^5.2.0",
        "nodemon": "^1.18.9"
    },
    "@dek/plugins": {
        "Express": "https://github.com/dekproject/express",
        "MongoDB": "https://github.com/dekproject/mongodb",
        "Redis": "https://github.com/dekproject/redis",
        "Authentication": "https://github.com/dekproject/authentication",
        "Elasticsearch": "https://github.com/dekproject/elasticsearch",
        "TensorFlow": "https://github.com/dekproject/tensorflow",
        "SendGrid": "https://github.com/dekproject/sendgrid"
    },
    "@dek/scripts": {
        "cliDevMode": "sudo npm i -g nodemon",
        "devMode": "babel-cli babel-preset-env babel-preset-minify nodemon mocha cross-env --save-dev",
        "webpack": "webpack webpack-cli webpack-dev-server babel-core babel-loader babel-preset-es2015 babili-webpack-plugin --save-dev",
        "webpackLoaders": "css-loader style-loader url-loader file-loader --save-dev",
    },
    "@dek/frontend": {
        "nuxt": "npx create-nuxt-app public",
        "angular": "sudo npm install -g @angular/cli && ng new public",
        "react": "npx create-react-app public",
        "ember": "sudo npm install -g ember-cli && ember new public"
    }
}
